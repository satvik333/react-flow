{
  "name": "react-flow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/styles": "^4.11.5",
    "@material-ui/system": "^4.12.2",
    "@material-ui/utils": "4.11.3",
    "@mui/icons-material": "^5.15.16",
    "@mui/material": "^5.15.16",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "classnames": "^2.5.1",
    "moment": "^2.30.1",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^9.5.4",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "react-window": "^1.8.10",
    "tailwindcss": "^1.9.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "yarn run tailwind && react-scripts start",
    "build": "yarn run tailwind && node ./purge-tailwindcss.js && cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "tailwind": "./node_modules/.bin/tailwind build ./src/tailwind-config.css -c ./tailwind.config.js -o ./src/styles/tailwind.css",
    "purge-tailwind": "node ./purge-tailwindcss.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-docs": "find ./src/app/main/documentation/material-ui-components \\( -name '*.ts' -o -name '*.tsx' \\) -type f -delete && babel-node src/app/main/documentation/material-ui-components/build.js",
    "build-stats": "yarn run build --stats",
    "bundle-report": "yarn run build-stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "lint": "eslint",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "yarn run tailwind && node ./purge-tailwindcss.js && build-storybook -s public",
    "chromatic": "yarn chromatic --project-token=ns4i1x8rdt",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
